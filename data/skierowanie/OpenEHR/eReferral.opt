                        <term_definitions code="at0000">
                            <items id="text">Problem</items>
                            <items id="description">An issue or obstacle which adversely impacts on the physical, mental and/or social well-being of an individual. The definition of a problem is deliberately kept rather loose so as to capture any real or perceived concerns that may adversely affect an individual's wellbeing to any degree.  </items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">structure</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
