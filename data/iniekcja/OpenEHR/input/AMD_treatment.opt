                        <archetype_id>
                            <value>openEHR-EHR-ACTION.medication.v0</value>
                        </archetype_id>
                        <term_definitions code="at0000">
                            <items id="text">Medication</items>
                            <items id="description">Details of use, administration, dispensing or other care step relating to a medicine, vaccine or other therapeutic good which may arise from an instruction from a clinician.</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Prescription first issued</items>
                            <items id="description">The instruction has issued as an item on a prescription.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Medication dispensed</items>
                            <items id="description">The medication has been dispensed.</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Medication course commenced</items>
                            <items id="description">The medication has been taken by, or administered to, the patient for the first time.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Medication course reviewed</items>
                            <items id="description">The medication has been reviewed.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Dose administered</items>
                            <items id="description">A single administration of the medication has taken place.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Medication course completed</items>
                            <items id="description">The medication course has been completed as planned.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Medication course supply delayed</items>
                            <items id="description">The medication course has not been dispensed as supply is not yet available.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Medication course suspended</items>
                            <items id="description">The administration of the medication has been suspended. No further doses should be given until further notice, or when the restart date or conditions have been met.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Prescription re-issued</items>
                            <items id="description">A prescription token has been re-issued for an existing medication order.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Prescription re-issue suspended</items>
                            <items id="description">Reissuing of this prescription is not available without some intervention.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Medication course cancelled</items>
                            <items id="description">The planned administration has been cancelled prior to any administration.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Scheduled medication postponed</items>
                            <items id="description">A scheduled medication has been postponed prior to any administration.</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Medication course stopped</items>
                            <items id="description">The administration of this medication has been ceased before anticipated completion.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Medication start date/condition set</items>
                            <items id="description">The time to start this medication, or other starting condition, has been set.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Dose administration omitted</items>
                            <items id="description">The administration of a medication has been withheld and not given. There is no expectation that it will be given later, though the next dose (if any) should be administered according to the original order.</items>
                        </term_definitions>
                        <term_definitions code="at0020">
                            <items id="text">Medication</items>
                            <items id="description">The medicine, vaccine or other therapeutic good which was the focus of the action. This element will normally be coded with a medicines terminology but free text may be required in some cases.</items>
                            <items id="comment">For example: 'Atenolol 100mg' or 'Atenolol tablets 100mg'.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Reason</items>
                            <items id="description">The reason(s) the specific action or step was carried out. Note: This is not the reason for the medication instruction, rather the specific reason e.g. for administration.</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">Comment</items>
                            <items id="description">A comment on the action taken including details of any variance between the intended action and the action actually performed.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Sequence number</items>
                            <items id="description">The sequence number specific to the action being recorded.</items>
                        </term_definitions>
                        <term_definitions code="at0030">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0033">
                            <items id="text">Patient guidance</items>
                            <items id="description">Any guidance, instructions or advice given to the subject of care or carer at the time of the action.</items>
                        </term_definitions>
                        <term_definitions code="at0035">
                            <items id="text">Dispensed medication returned.</items>
                            <items id="description">A dispensed medication is returned, or not collected, without being used.</items>
                        </term_definitions>
                        <term_definitions code="at0039">
                            <items id="text">Major change to order.</items>
                            <items id="description">A major change to the order was required, resulting in this order being stopped and a replacement order being started.</items>
                        </term_definitions>
                        <term_definitions code="at0041">
                            <items id="text">Minor change to order</items>
                            <items id="description">The medication order has been changed in a manner which does not require a new instruction/order to be issued, according to local clinical rules.</items>
                        </term_definitions>
                        <term_definitions code="at0043">
                            <items id="text">Order scheduled datetime</items>
                            <items id="description">The datetime at which the medication order was scheduled to occur.</items>
                            <items id="comment"> Used to compare variance from actual action time where this is not readily calculable from the original instruction.</items>
                        </term_definitions>
                        <term_definitions code="at0044">
                            <items id="text">Dose administration deferred</items>
                            <items id="description">Administration of a dose of mediciation has been delayed but is expected to be given as soon as possible.</items>
                        </term_definitions>
                        <term_definitions code="at0053">
                            <items id="text">Details</items>
                            <items id="description">Further structured details of the action, possibly specific to a pathway step.</items>
                        </term_definitions>
                        <term_definitions code="at0082">
                            <items id="text">Product</items>
                            <items id="description">Details of the product dispensed, authorised or administered.</items>
                        </term_definitions>
                        <term_definitions code="at0083">
                            <items id="text">Batch ID</items>
                            <items id="description">Assigned by the manufacturer to identify the manufacturing batch of the item.</items>
                        </term_definitions>
                        <term_definitions code="at0084">
                            <items id="text">Expiry date</items>
                            <items id="description">The expiry date of the medicine administered as documented by the manufacturer.</items>
                        </term_definitions>
                        <term_definitions code="at0085">
                            <items id="text">Extension</items>
                            <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
                            <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
                        </term_definitions>
                        <term_definitions code="at0103">
                            <items id="text">Order ID</items>
                            <items id="description">The identifier assigned with the original order.</items>
                        </term_definitions>
                        <term_definitions code="at0104">
                            <items id="text">Preparation details</items>
                            <items id="description">Details of the medication preparation.</items>
                        </term_definitions>
                        <term_definitions code="at0105">
                            <items id="text">Max class dose administered</items>
                            <items id="description">The proportion of the maximum allowed dose equivalent administered.</items>
                            <items id="comment">Used in mental health to allow monitoring of total amount of a drug class andministered e.g. anti-psychotics, where each medication is associated with a percentage contribution towards a total daily maximum, normally 100%.</items>
                        </term_definitions>
                        <term_definitions code="at0106">
                            <items id="text">Medication re-authorised</items>
                            <items id="description">A repeat prescription or dispense has been re-authorised.</items>
                        </term_definitions>
                        <term_definitions code="at0109">
                            <items id="text">Medication recommended</items>
                            <items id="description">The medication has been recommended but no steps have been taken to initiate prescribing.</items>
                        </term_definitions>
                        <term_definitions code="at0116">
                            <items id="text">Reason dose not administered</items>
                            <items id="description">The reason(s) why a particular dose administration did not take place, including complte omission of the dose and deferral of the dose.</items>
                        </term_definitions>
                        <term_definitions code="at0117">
                            <items id="text">Reason course amended/stopped</items>
                            <items id="description">The reason(s) for amending or stopping a course of medication.</items>
                        </term_definitions>
                        <term_definitions code="at0118">
                            <items id="text">Allergy/Adverse Reaction/ Intolerance</items>
                            <items id="description">The patient experienced an adverse reaction.</items>
                        </term_definitions>
                        <term_definitions code="at0119">
                            <items id="text">Inadequate clinical response</items>
                            <items id="description">The clinical response to the medication was inadequate.</items>
                        </term_definitions>
                        <term_definitions code="at0120">
                            <items id="text">Patient choice / difficulty</items>
                            <items id="description">The course was changed because of patient preference or difficulty.</items>
                        </term_definitions>
                        <term_definitions code="at0121">
                            <items id="text">Prescribed in error</items>
                            <items id="description">The medication was prescribed in error.</items>
                        </term_definitions>
                        <term_definitions code="at0122">
                            <items id="text">Additional information available</items>
                            <items id="description">Addiotnal information informed the amendment.</items>
                        </term_definitions>
                        <term_definitions code="at0123">
                            <items id="text">Temporarily suspended</items>
                            <items id="description">The medication was suspended for a temporary period only.</items>
                        </term_definitions>
                        <term_definitions code="at0124">
                            <items id="text">Change to dose form/ strength /route</items>
                            <items id="description">The dose form/strength and or route were changed.</items>
                        </term_definitions>
                        <term_definitions code="at0125">
                            <items id="text">Patient refused</items>
                            <items id="description">The patient refused to take the medication.</items>
                        </term_definitions>
                        <term_definitions code="at0126">
                            <items id="text">Nil by mouth</items>
                            <items id="description">The patient is on a nil-by-mouth regime.</items>
                        </term_definitions>
                        <term_definitions code="at0127">
                            <items id="text">Medicine unavailable</items>
                            <items id="description">The medicine was unavailable.</items>
                        </term_definitions>
                        <term_definitions code="at0128">
                            <items id="text">Patient unavailable</items>
                            <items id="description">The patient was not available.</items>
                        </term_definitions>
                        <term_definitions code="at0129">
                            <items id="text">Clinical reason</items>
                            <items id="description">The medication administration was withheld/deferred for a clinical reason.</items>
                        </term_definitions>
                        <term_definitions code="at0130">
                            <items id="text">Manufacturer</items>
                            <items id="description">The manufacturer of the medication.</items>
                        </term_definitions>
                        <term_definitions code="at0131">
                            <items id="text">Amount</items>
                            <items id="description">Details of the amount of medication supplied i.e. authorised, dispensed or administered.</items>
                        </term_definitions>
                        <term_definitions code="at0132">
                            <items id="text">Dispensing substitution</items>
                            <items id="description">Subsitution action taken by the dispenser.</items>
