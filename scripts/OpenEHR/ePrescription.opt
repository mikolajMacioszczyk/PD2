        <term_definitions code="at0000">
            <items id="text">Authorisation details</items>
            <items id="description">Details of the authorisation of a medicine, vaccine or other therapeutic good.</items>
        </term_definitions>
        <term_definitions code="at0025">
            <items id="text">numberOfRepeatsAllowed</items>
            <items id="description">The number of times the expressed quantity of medicine, vaccine or other therapeutic good may be refilled or redispensed without a new prescription.</items>
        </term_definitions>
        <term_definitions code="at0046">
            <items id="text">Minimum interval between refills</items>
            <items id="description">The minimum time between repeat supply of the medicine, vaccine or therapeutic good. Note: This is specified by the ordering clinician for a specific reason such as safety or best practice.</items>
        </term_definitions>
        <term_definitions code="at0072">
            <items id="text">validityPeriod</items>
            <items id="description">The repeat supply authorisation has expired after this date.</items>
        </term_definitions>
        <term_definitions code="at0073">
            <items id="text">Authorisation type</items>
            <items id="description">Whether the medication is only issued once or may re-issued and dispensed 're-filled' after authorisation by the prescriber.</items>
        </term_definitions>
        <term_definitions code="at0074">
            <items id="text">No repeat supply</items>
            <items id="description">Repeat supply has not been authorised.</items>
        </term_definitions>
        <term_definitions code="at0075">
            <items id="text">Repeat prescribing</items>
            <items id="description">Multiple refills of the prescription may be obtained from the prescriber.</items>
        </term_definitions>
        <term_definitions code="at0076">
            <items id="text">Repeat dispensing</items>
            <items id="description">Multiple refills of the prescription may be obtained from the dispenser.</items>
        </term_definitions>
        <term_definitions code="at0078">
            <items id="text">Number of refills issued</items>
            <items id="description">The number of refills which have been issued or dispensed for this period of authorisation.</items>
        </term_definitions>
        <term_definitions code="at0079">
            <items id="text">Number of refills remaining</items>
            <items id="description">The number of re-fills or re-issues that remain valid for this authorisation period.</items>
        </term_definitions>
        <term_definitions code="at0080">
            <items id="text">Prescriber endorsement</items>
            <items id="description">An endorsement by the prescriber that the medication may be supplied under a specific contractual arrangement.</items>
            <items id="comment">e.g. Some medications may be supplied only to certain patients in specifc circumstances, or may be made avaialble free-of-charge. It is common for prescribers to be asked to endorse the prescription to that effect. </items>
        </term_definitions>
